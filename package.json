{
  "name": "gymhub-final-backend",
  "version": "2.0.1",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec node --loader ts-node/esm src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "node --loader ts-node/esm prisma/seed.ts",
    "prisma:validate": "prisma validate"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "nodemon": "^3.1.4"
  }
}